{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook-proxy.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook-proxy",
      "dest": "/api/webhook-proxy.py"
    },
    {
      "src": "/api/webhook-proxy/health",
      "dest": "/api/webhook-proxy.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}